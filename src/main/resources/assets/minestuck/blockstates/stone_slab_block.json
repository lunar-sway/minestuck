        },
        {
            "when":  { "facing": "south", "carved": "false" },
            "apply": { "model": "minestuck:block/stone_slab_block", "y": 180 }
        },
        {
            "when":  { "facing": "west", "carved": "false" },
            "apply": { "model": "minestuck:block/stone_slab_block", "y": 270 }
        },
        {
            "when":  { "facing": "east", "carved": "false" },
            "apply": { "model": "minestuck:block/stone_slab_block", "y": 90 }
        },
        {
            "when":  { "facing": "north", "carved": "true" },
            "apply": { "model": "minestuck:block/stone_slab_written_block" }
        },
        {
            "when":  { "facing": "south", "carved": "true" },
            "apply": { "model": "minestuck:block/stone_slab_written_block", "y": 180 }
        },
        {
            "when":  { "facing": "west", "carved": "true" },
            "apply": { "model": "minestuck:block/stone_slab_written_block", "y": 270 }
        },
        {
            "when":  { "facing": "east", "carved": "true" },
            "apply": { "model": "minestuck:block/stone_slab_written_block", "y": 90 }
