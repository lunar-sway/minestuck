{
    "multipart": [
        {
            "when":  { "facing": "north", "open": "false" },
            "apply": { "model": "minestuck:block/cassette_player_closed" }
        },
        {
            "when":  { "facing": "south", "open": "false" },
            "apply": { "model": "minestuck:block/cassette_player_closed", "y": 180 }
        },
        {
            "when":  { "facing": "west", "open": "false" },
            "apply": { "model": "minestuck:block/cassette_player_closed", "y": 270 }
        },
        {
            "when":  { "facing": "east", "open": "false" },
            "apply": { "model": "minestuck:block/cassette_player_closed", "y": 90 }
        },
        {
            "when":  { "facing": "north", "open": "true" },
            "apply": { "model": "minestuck:block/cassette_player_open" }
        },
        {
            "when":  { "facing": "south", "open": "true" },
            "apply": { "model": "minestuck:block/cassette_player_open", "y": 180 }
        },
        {
            "when":  { "facing": "west", "open": "true" },
            "apply": { "model": "minestuck:block/cassette_player_open", "y": 270 }
        },
        {
            "when":  { "facing": "east", "open": "true" },
            "apply": { "model": "minestuck:block/cassette_player_open", "y": 90 }
        },
        {   "when": { "facing": "north", "cassette": "true" },
            "apply": { "model": "minestuck:block/cassette/cassette"}
        },
        {   "when": { "facing": "south", "cassette": "true" },
            "apply": { "model": "minestuck:block/cassette/cassette", "y": 180 }
        },
        {   "when": { "facing": "west", "cassette": "true" },
            "apply": { "model": "minestuck:block/cassette/cassette", "y": 270 }
        },
        {   "when": { "facing": "east", "cassette": "true" },
            "apply": { "model": "minestuck:block/cassette/cassette", "y": 90 }
        }
    ]
}
